        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.34",
        "mongoose": "^5.13",
        "pm2": "^5.2.0",
        "uuid-random": "^1.3.2",
        "winston": "^3.6.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.36",
        "@types/bull": "^3.15.8",
        "@types/dotenv-safe": "^8.1.2",
