    "private": true,
    "scripts": {
        "build": "yarn clean && tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
        "clean": "rimraf dist",
        "dev": "nodemon src/index.ts",
        "dev-worker": "nodemon src/index-worker.ts",
        "dev-consumer": "nodemon src/index-consumer.ts",
        "docker:start": "pm2-docker start ./dist/index.js",
        "docker:start-worker": "pm2-docker start ./dist/index-worker.js",
        "docker:start-consumer": "pm2-docker start ./dist/index-consumer.js"
    },
